{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "standard": {
        "format": "%(asctime)s [%(levelname)s]: %(message)s"
      }
    },
    "handlers": {
      "default": {
        "level": "INFO",
        "formatter": "standard",
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "Root": {
        "handlers": ["default"],
        "level": "DEBUG",
        "propagate": "False"
      }
    }
  }